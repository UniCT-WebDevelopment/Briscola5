{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "node_modules"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "remoteFileExplorer",
    "footer": "Created by Simon Speich",
    "copyright": "2013",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "recurseDepth": 10

  }
}